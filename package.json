    "type": "git",
    "url": "git+https://github.com/MJones180/PhenominalServer"
  },
  "dependencies": {
    "apollo-errors": "^1.9.0",
    "async": "^2.6.0",
    "aws-sdk": "^2.382.0",
    "chalk": "^2.4.0",
    "dotenv": "5.0.1",
    "email-validator": "^2.0.4",
    "fbgraph": "^1.4.4",
    "google-auth-library": "^1.3.2",
    "graphql-yoga": "^1.16.9",
    "jsonfile": "^1.0.5",
    "jsonwebtoken": "8.2.0",
    "lodash": "^4.17.5",
    "minimist": "^1.2.0",
    "moment": "^2.21.0",
    "node-mailjet": "^3.2.1",
    "prisma-binding": "2.2.12",
    "prisma-client-lib": "^1.23.2",
    "stripe": "^5.6.0"
  },
  "devDependencies": {
    "eslint": "^4.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "graphql-cli": "^3.0.3",
    "nodemon": "1.17.2"
  },
  "scripts": {
    "deploy": "prisma deploy --env-file .env.dev && prisma generate --env-file .env.dev",
    "deployProd": "prisma deploy --env-file .env.prod && prisma generate --env-file .env.prod",
    "playground": "graphql playground",
    "pushEvents": "node run --script=pushEvents",
    "pushEventsProd": "node run --script=pushEvents --env=prod",
    "start": "nodemon -e js,graphql -x node run --script=start",
    "syncCharities": "node run --script=syncCharities && node run --script=syncCharities --env=prod"
  }
